generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Events {
  id          Int      @id @default(autoincrement())
  ticket      Int?
  slug        String?  @unique
  title       String
  location    String
  publishedAt DateTime @default(now())

  @@map("events")
}

model Tickets {
  id        Int      @id @default(autoincrement())
  slug      String?  @unique
  name      String
  quantity  Int
  location  String
  appliedAt DateTime @default(now())

  @@map("tickets")
}
